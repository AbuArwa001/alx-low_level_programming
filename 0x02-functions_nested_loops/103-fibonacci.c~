#include <stdio.h>
/**
 * main - prints fibonanci sequence
 *
 *
 * Return: always retunr 0
 *
 **/
int main(void)
{	unsigned long int sum = 2, p = 1, cur = 2, nxt = 0, tmp = 0;

	while (nxt <= 4000000)
	{	nxt = p + cur;
		if ( nxt % 2 == 0)
		{
			sum += sum;

		}
		tmp = cur;
		cur = nxt;
		p = tmp;
	}
	printf("%ld\n", sum);
	return (0);
}
